<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	
  <properties resource="jdbc.properties"/>

  
  <environments default="development">
    <environment id="development">
      <transactionManager type="JDBC"/>
      <dataSource type="POOLED">
        <property name="driver" value="${jdbc.driver}"/>
        <property name="url" value="${jdbc.url}"/>
        <property name="username" value="${jdbc.user}"/>
        <property name="password" value="${jdbc.password}"/>
      </dataSource>
    </environment>
  </environments>
  
 
  <mappers>

<!--    <package name="com.bjpowernode.crm.settings.dao"/>-->
<!--    <package name="com.bjpowernode.crm.workbench.dao"/>-->
      <mapper resource="com/bjpowernode/crm/settings/dao/UserDao.xml"></mapper>
      <mapper resource="com/bjpowernode/crm/settings/dao/DicValueDao.xml"></mapper>
      <mapper resource="com/bjpowernode/crm/settings/dao/DicTypeDao.xml"></mapper>
      <mapper resource="com/bjpowernode/crm/workbench/dao/ActivityDao.xml"></mapper>
      <mapper resource="com/bjpowernode/crm/workbench/dao/ActivityRemarkDao.xml"></mapper>
      <mapper resource="com/bjpowernode/crm/workbench/dao/ClueDao.xml"></mapper>
      <mapper resource="com/bjpowernode/crm/workbench/dao/ClueActivityRelationDao.xml"></mapper>
      <mapper resource="com/bjpowernode/crm/workbench/dao/ClueRemarkDao.xml"></mapper>
      <mapper resource="com/bjpowernode/crm/workbench/dao/ContactsActivityRelationDao.xml"></mapper>
      <mapper resource="com/bjpowernode/crm/workbench/dao/ContactsDao.xml"></mapper>
      <mapper resource="com/bjpowernode/crm/workbench/dao/ContactsRemarkDao.xml"></mapper>
      <mapper resource="com/bjpowernode/crm/workbench/dao/CustomerDao.xml"></mapper>
      <mapper resource="com/bjpowernode/crm/workbench/dao/CustomerRemarkDao.xml"></mapper>
      <mapper resource="com/bjpowernode/crm/workbench/dao/TranDao.xml"></mapper>
      <mapper resource="com/bjpowernode/crm/workbench/dao/TranHistoryDao.xml"></mapper>


  </mappers>
</configuration>

































